
CREATE DATABASE login ;

CREATE TABLE `login`.`daskaloi` ( `id` INT(50) NOT NULL AUTO_INCREMENT , `username` VARCHAR(50) NOT NULL , `password` VARCHAR(50) NOT NULL , `onoma` VARCHAR(50) NOT NULL , `epitheto` VARCHAR(50) NOT NULL , `email` VARCHAR(50) NOT NULL , PRIMARY KEY (`id`)) ENGINE = InnoDB;

